	<queryString>
		<![CDATA[select
mbmembmaster.member_no ,
mbucfprename.prename_desc ,
mbmembmaster.memb_name ,
mbmembmaster.memb_surname ,
mbucfprovince.province_desc ,
mbucfdistrict.district_desc ,
nvl(mbucftambol.tambol_desc ,0 ) as tambol_desc ,
nvl(mbmembmaster.curraddr_no , 0 ) as addr_no ,
nvl(mbmembmaster.curraddr_soi , 0 ) as addr_soi ,
nvl(mbmembmaster.curraddr_road , 0 ) as addr_road ,
nvl(mbmembmaster.curraddr_moo , 0 ) as addr_moo ,
nvl(mbmembmaster.curraddr_village , 0 ) as addr_village ,
nvl(mbmembmaster.curraddr_postcode , 0 ) as addr_postcode
from mbmembmaster
left join mbucfprename mbprename on mbmembmaster.prename_code = mbprename.prename_code
left join mbucfprename on mbmembmaster.prename_code = mbucfprename.prename_code
left join mbucfprovince on mbmembmaster.currprovince_code = mbucfprovince.province_code
left join mbucfdistrict on mbmembmaster.curramphur_code = mbucfdistrict.district_code
left join mbucftambol on mbmembmaster.currtambol_code = mbucftambol.tambol_code
where mbmembmaster.coop_id = $P{as_coopid}
and mbmembmaster.member_no like $P{as_membno}]]>
	</queryString>
