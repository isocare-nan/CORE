	<parameter name="as_smembno" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
mm.member_no,dm.depttype_code,prncbal ,dt.depttype_desc,dm.deptaccount_no
from dpdeptmaster dm
left join mbmembmaster mm on mm.member_no = dm.member_no
left join dpdepttype dt on dm.depttype_code =dt.depttype_code
where deptclose_status = 0
and  mm.member_no =$P{as_smembno}
group by mm.member_no,dm.depttype_code,prncbal ,dt.depttype_desc,dm.deptaccount_no
order by dm.depttype_code,dm.deptaccount_no
]]>
	</queryString>
