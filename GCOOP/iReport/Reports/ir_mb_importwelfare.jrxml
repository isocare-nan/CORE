	<queryString>
		<![CDATA[select
wfm.member_no,
pe.prename_desc ,
mb.memb_name,
mb.memb_surname,
wfm.welfare_amt,
ucw.welfare_desc,
wfm.welfare_date
from mbwelfaremaster wfm
left join mbmembmaster mb on wfm.member_no = mb.member_no
left join mbucfprename pe on mb.prename_code = pe.prename_code
left join mbucfwelfare ucw on ucw.welfare_type = wfm.welfare_type
where wfm.coop_id = $P{as_coopid}
and wfm.welfare_date = $P{adtm_date}]]>
	</queryString>
