	<queryString>
		<![CDATA[SELECT "LNREQLOAN"."LOANREQUEST_DOCNO",
         "MBMEMBMASTER"."MEMBER_NO", lnreqloan.loanrcvfix_date  ,
         "LNREQLOAN"."LOANREQUEST_DATE",
         "MBUCFPRENAME"."PRENAME_DESC",
         "MBMEMBMASTER"."MEMB_NAME",
         "MBMEMBMASTER"."MEMB_SURNAME",
         "MBUCFMEMBGROUP_A"."MEMBGROUP_DESC",
         "MBMEMBMASTER"."BIRTH_DATE",
         "MBMEMBMASTER"."LEVEL_CODE",
         "MBMEMBMASTER"."WORK_DATE",
         "MBMEMBMASTER"."MEMBER_DATE",
         "MBMEMBMASTER"."SALARY_AMOUNT",
         "SHSHAREMASTER"."SHARESTK_AMT",
         "SHSHARETYPE"."SHARE_VALUE",
         "SHSHAREMASTER"."PERIODSHARE_AMT",
         "LNREQLOAN"."LOANREQUEST_AMT",
         "LNREQLOAN"."PERIOD_PAYAMT",
         "LNREQLOAN"."PERIOD_PAYMENT",
         "LNREQLOAN"."REMARK",
         "LNREQLOAN"."EXPENSE_CODE",
         "LNUCFLOANOBJECTIVE"."LOANOBJECTIVE_DESC",
         "LNREQLOAN"."LOANTYPE_CODE",
         "LNLOANTYPE"."LOANTYPE_DESC",
         "LNREQLOAN"."LOANPERMISS_AMT",
         "LNREQLOAN"."LOANCREDIT_AMT",
         "LNREQLOAN"."LOANPAYMENT_TYPE",
         "LNREQLOAN"."LOANREQREGIS_AMT",
         "LNLOANTYPE"."PREFIX",
         "LNREQLOAN"."OTHERCLR_AMT",
         "LNREQLOAN"."BUYSHARE_AMT",
         "LNREQLOAN"."SUM_CLEAR",
         "MBUCFMEMBGROUP_A"."MEMBGROUP_CONTROL",
         "LNREQLOAN"."LOANINS_AMT",
         "LNLOANTYPE"."LOANGROUP_CODE",
         "MBMEMBMASTER"."MEMBGROUP_CODE",
         "LNREQLOAN"."LOANCONTRACT_NO",
         "MBUCFMEMBGROUP_B"."MEMBGROUP_DESC",
         "MBUCFMEMBGROUP_C"."MEMBGROUP_CODE",
         "MBUCFMEMBGROUP_C"."MEMBGROUP_DESC",
( select
sum(nvl(lm.principal_balance,0)) as sum_principal
from lnreqloanclr clr
 left join lncontmaster lm on clr.loancontract_no = lm.loancontract_no
left join lnreqloanclrother oth on clr.loanrequest_docno = oth.loanrequest_docno
where clr.clear_status = '1'
and clr.loanrequest_docno = "LNREQLOAN"."LOANREQUEST_DOCNO"
and lm.member_no =  "MBMEMBMASTER"."MEMBER_NO"
) as sum_principal ,
( select
sum(nvl(lm.rkeep_principal,0)) as sum_rkeep
from lnreqloanclr clr
 left join lncontmaster lm on clr.loancontract_no = lm.loancontract_no
left join lnreqloanclrother oth on clr.loanrequest_docno = oth.loanrequest_docno
where clr.clear_status = '1'
and clr.loanrequest_docno = "LNREQLOAN"."LOANREQUEST_DOCNO"
and lm.member_no =  "MBMEMBMASTER"."MEMBER_NO"
) as sum_rkeep ,
( select
sum(nvl(lm.interest_arrear,0)) as sum_interest
from lnreqloanclr clr
 left join lncontmaster lm on clr.loancontract_no = lm.loancontract_no
left join lnreqloanclrother oth on clr.loanrequest_docno = oth.loanrequest_docno
where clr.clear_status = '1'
and clr.loanrequest_docno = "LNREQLOAN"."LOANREQUEST_DOCNO"
and lm.member_no =  "MBMEMBMASTER"."MEMBER_NO"
) as sum_interest ,
( select
sum(nvl(oth.clrother_amt,0)) as sum_clrother
from lnreqloanclr clr
 left join lncontmaster lm on clr.loancontract_no = lm.loancontract_no
left join lnreqloanclrother oth on clr.loanrequest_docno = oth.loanrequest_docno
where clr.clear_status = '1'
and clr.loanrequest_docno = "LNREQLOAN"."LOANREQUEST_DOCNO"
and lm.member_no =  "MBMEMBMASTER"."MEMBER_NO"
) as sum_clrother ,

( select
(
nvl( (select sum(b.principal_balance) from lnreqloanclr b where  f.loanrequest_docno = b.loanrequest_docno and b.clear_status = '1' ) ,0) +
nvl( (select sum(c.interest_arrear) from lnreqloanclr c,lnreqloan cc where  c.loanrequest_docno = cc.loanrequest_docno and  f.loanrequest_docno = c.loanrequest_docno and  c.clear_status = '1'  ) ,0) +
nvl( (select sum(d.intestimate_amt) from lnreqloanclr d where  f.loanrequest_docno = d.loanrequest_docno and d.clear_status = '1' and d.rkeep_interest = 0) ,0) +
nvl( (select sum(e.clrother_amt) from lnreqloanclrother e where  f.loanrequest_docno = e.loanrequest_docno) ,0) -
nvl( (select sum(a.rkeep_principal) from lnreqloanclr a where  f.loanrequest_docno = a.loanrequest_docno and a.clear_status = '1' ) ,0)
 ) as sum_total
from lnreqloan f
where f.loanrequest_docno=  "LNREQLOAN"."LOANREQUEST_DOCNO" ) as sum_total ,

		  ftcm_calagemth (birth_date,sysdate) as "memb_age",
		  ftcm_calagemth (member_date,sysdate) as "member_age",
nvl((select sum(lnreqloanclrother.clrother_amt) as sum_ins  from lnreqloanclrother where lnreqloanclrother.clrothertype_code = 'INS' and lnreqloanclrother.loanrequest_docno = LNREQLOAN.LOANREQUEST_DOCNO),0) as sum_ins,
nvl((select sum(lnreqloanclrother.clrother_amt) as sum_shr from lnreqloanclrother where lnreqloanclrother.clrothertype_code = 'SHR' and lnreqloanclrother.loanrequest_docno = LNREQLOAN.LOANREQUEST_DOCNO ),0) as sum_shr,
nvl((select sum(lnreqloanclrother.clrother_amt) as sum_ssc from lnreqloanclrother where lnreqloanclrother.clrothertype_code = 'SSC' and lnreqloanclrother.loanrequest_docno = LNREQLOAN.LOANREQUEST_DOCNO),0) as sum_ssc ,
nvl((select sum(lnreqloanclrother.clrother_amt) as sum_isc from lnreqloanclrother where lnreqloanclrother.clrothertype_code = 'ISC' and lnreqloanclrother.loanrequest_docno = LNREQLOAN.LOANREQUEST_DOCNO),0) as sum_isc ,
nvl((select sum(lnreqloanclrother.clrother_amt) as sum_paw from lnreqloanclrother where lnreqloanclrother.clrothertype_code = 'PAW' and lnreqloanclrother.loanrequest_docno = LNREQLOAN.LOANREQUEST_DOCNO),0) as sum_paw

    FROM "LNREQLOAN",
         "MBMEMBMASTER",
         "MBUCFPRENAME",
         "MBUCFMEMBGROUP" "MBUCFMEMBGROUP_A",
         "SHSHAREMASTER",
         "SHSHARETYPE",
         "LNUCFLOANOBJECTIVE",
         "LNLOANTYPE",
         "MBUCFMEMBGROUP" "MBUCFMEMBGROUP_B",
         "MBUCFMEMBGROUP" "MBUCFMEMBGROUP_C"
   WHERE ( mbucfmembgroup_a.membgroup_control = mbucfmembgroup_b.membgroup_code (+)) and
         ( mbucfmembgroup_b.membgroup_control = mbucfmembgroup_c.membgroup_code (+)) and
         ( lnucfloanobjective.loanobjective_code (+) = lnreqloan.loanobjective_code) and
         ( lnreqloan.loantype_code = lnucfloanobjective.loantype_code (+)) and
         ( "MBMEMBMASTER"."MEMBER_NO" = "LNREQLOAN"."MEMBER_NO" ) and
         ( "MBMEMBMASTER"."PRENAME_CODE" = "MBUCFPRENAME"."PRENAME_CODE" ) and
         ( "MBUCFMEMBGROUP_A"."MEMBGROUP_CODE" = "MBMEMBMASTER"."MEMBGROUP_CODE" ) and
         ( "SHSHAREMASTER"."MEMBER_NO" = "MBMEMBMASTER"."MEMBER_NO" ) and
         ( "SHSHAREMASTER"."SHARETYPE_CODE" = "SHSHARETYPE"."SHARETYPE_CODE" ) and
         ( "LNREQLOAN"."LOANTYPE_CODE" = "LNLOANTYPE"."LOANTYPE_CODE" ) and
         ( ( lnreqloan.loanrcvfix_date between $P{adtm_startdate} and $P{adtm_enddate} ) AND
         ( lnreqloan.loantype_code between $P{as_startlntype} and $P{as_endlntype} ) AND
         ( lnreqloan.loanrequest_status =  $P{as_status} ) and lnreqloan.coop_id = $P{as_coopid} )
order by lnreqloan.loanrcvfix_date , lnreqloan.loantype_code , "LNREQLOAN"."LOANREQUEST_DOCNO"]]>
	</queryString>
