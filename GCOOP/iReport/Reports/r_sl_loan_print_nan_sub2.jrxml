	<queryString>
		<![CDATA[SELECT

LNCONTMASTER.coop_id,LNCONTMASTER.LOANCONTRACT_NO,LNREQLOANCOLL.REF_COLLNO,LNREQLOANCOLL.DESCRIPTION

from LNCONTMASTER
LEFT JOIN LNREQLOANCOLL on trim(LNCONTMASTER.loanrequest_docno) = trim(LNREQLOANCOLL.loanrequest_docno)
where trim(LNCONTMASTER.coop_id) = $P{coop_id}
and trim(LNCONTMASTER.LOANCONTRACT_NO) = $P{LOANCONTRACT_NO}
and LNREQLOANCOLL.REF_COLLNO is not null
order by LNREQLOANCOLL.REF_COLLNO,LNREQLOANCOLL.DESCRIPTION asc]]>
