	<queryString>
		<![CDATA[select
ln.loancontract_no as loancontract_no,
ln.member_no as member_no ,
prem.prename_desc as prename_desc,
mbm.memb_name as memb_name,
mbm.memb_surname as memb_surname,
lc.ref_collno as ref_collno,
prec.prename_desc as prename_c,
mbc.memb_name as name_c,
mbc.memb_surname as surname_c,
mbc.resign_date,
ln.principal_balance,
con.coop_name
from lncontmaster ln
left join lncontcoll lc on ln.coop_id = lc.coop_id and ln.loancontract_no	= lc.loancontract_no
left join mbmembmaster mbc on lc.ref_collno = mbc.member_no and mbc.coop_id = ln.coop_id
left join mbmembmaster mbm on ln.member_no = mbm.member_no and ln.coop_id = mbm.coop_id
left join mbucfprename prem on mbm.prename_code = prem.prename_code
left join mbucfprename prec on mbm.prename_code = prec.prename_code
left join cmcoopconstant con on con.coop_no = ln.coop_id
where ln.coop_id = $P{as_coopid}
and ln.contract_status = 1
and ln.principal_balance > 0
and mbc.resign_status = 1
and mbc.resign_date between $P{date1} and $P{date2}
order by ln.loancontract_no]]>
	</queryString>
