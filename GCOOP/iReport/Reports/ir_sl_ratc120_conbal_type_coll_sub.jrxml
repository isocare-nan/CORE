	<queryString>
		<![CDATA[select
lc.loancontract_no as loancontract_no,
lc.ref_collno,
lc.description,
mb.addr_mobilephone
from lncontcoll lc
left join mbmembmaster mb on lc.ref_collno = mb.member_no
where lc.coop_id = $P{as_coopid}
and trim(lc.loancontract_no) = trim($P{as_loancontractno})
and loancolltype_code = 01
order by lc.ref_collno]]>
