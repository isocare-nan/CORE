	<parameter name="as_smembno" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
mm.member_no,ty.loantype_code,ty.loantype_desc , sum(principal_balance) as principal_balance
from mbmembmaster mm
left join lncontmaster lm  on mm.member_no = lm.member_no
left join lnloantype ty on lm.loantype_code =ty.loantype_code
where principal_balance > 0
and  mm.member_no =$P{as_smembno}
group by mm.member_no,ty.loantype_code,ty.loantype_desc
order by ty.loantype_code]]>
	</queryString>
