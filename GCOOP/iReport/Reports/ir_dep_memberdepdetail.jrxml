	</parameter>
	<parameter name="as_coop" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
c.member_no, FT_MEMNAME(c.coop_id,c.member_no)as fullname ,FT_MEMGRP(c.coop_id,trim(c.membgroup_code))as membgroup_desc ,a.deptaccount_no,a.prncbal
from dpdeptstatement a
inner join (select max(seq_no) seq_no,deptaccount_no from dpdeptstatement where operate_date <= $P{adtm_date} group by deptaccount_no) b
 on a.deptaccount_no = b.deptaccount_no and a.seq_no = b.seq_no
inner join (select * from dpdeptmaster  ) c on a.deptaccount_no = c.deptaccount_no
left join mbmembmaster on mbmembmaster.member_no=c.member_no
where
((c.deptclose_status = 0  )or (c.deptclose_status = 1  and c.deptclose_date > $P{adtm_date} ))
and a.prncbal > 0
order by  c.member_no,a.deptaccount_no]]>
	</queryString>
