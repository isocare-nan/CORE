	<queryString>
		<![CDATA[SELECT DISTINCT ASSREQMASTER.ASSIST_DOCNO,ASSREQMASTER.MEMBER_NO,
		ft_getmbname(ASSREQMASTER.COOP_ID , ASSREQMASTER.MEMBER_NO)  as memb_name,
		ASSREQMASTER.REQ_DATE,
         ASSREQMASTER.DEC_DEADDATE,
         ASSREQMASTER.DEC_CAUSE,
asg.GAIN_NAME, md.GAIN_CONCERN,asg.SEQ_NO
, CASE WHEN asg.ASSIST_DOCNO is null  THEN 0 ELSE 1 END as condition
    FROM ASSREQMASTER
left join ASSREQMASTERGAIN asg on ASSREQMASTER.ASSIST_DOCNO = asg.ASSIST_DOCNO
left join  MBUCFGAINCONCERN  md  on asg.GAINRELATION_CODE = md.CONCERN_CODE
   WHERE ( ASSREQMASTER.REQ_STATUS <> -9 )
and  ASSREQMASTER.MEMBER_NO =$P{as_membno}
order by ASSREQMASTER.ASSIST_DOCNO,asg.SEQ_NO]]>
	</queryString>
