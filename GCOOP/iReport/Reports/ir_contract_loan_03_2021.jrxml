	<queryString>
		<![CDATA[select
ln.loanrequest_docno,
ln.loancontract_no as loancontract_no,
ln.member_no,
mpn.prename_desc as prenamemem,
mb.memb_name,
mb.memb_surname,
mb.card_person,
mbs.position_desc,
mg.membgroup_desc,
mb.salary_amount,
mb.mate_name,
mbt.tambol_desc as tambol_mem,
mbd.district_desc as district_mem,
mbp.province_desc as province_mem,
mb.addr_postcode as postcode_mem,
mb.addr_mobilephone as addr_mobilephone,
mb.addr_email as addr_email,
mb.addr_no,
mb.addr_moo,
mb.addr_soi,
mb.addr_village,
mb.addr_road,
mb.salary_id,
mgt.tambol_desc as tambol_group,
mgd.district_desc as district_group,
mgp.province_desc as province_group,
mg.addr_postcode as postcode_group,
ln.loanapprove_amt,
FT_READTBAHT(ln.loanapprove_amt) as READTBAHT,
FT_READTBAHT(ln.period_payment) as READTBAHT_PAYMENT,
FTREADTNUM(ln.period_payamt) as READTBAHT_PAYAMT,
FT_calagemth(mb.birth_date,sysdate) as age,
lno.loanobjective_desc,
ln.period_payment ,
ln.period_payamt ,
lnd.interest_rate,
ln.loanpayment_type,
ln.startcont_date as startcont_date,
case when extract(day from ln.startcont_date ) <= AA.postingdate then add_months( ln.startcont_date , 1)  else add_months( ln.startcont_date , 2) end as next_date,
us.full_name as users_name,
case mariage_status when 1 then 'โสด' when 2 then'สมรส' when 3 then 'หย่าร้าง' when 4 then 'หม้าย' else '' end as mariage_status
from lncontmaster ln
left join lnloantype lty on ln.loantype_code = lty.loantype_code
left join mbmembmaster mb on ln.member_no = mb.member_no
left join mbucfprename mpn on mb.prename_code = mpn.prename_code
left join mbucfposition mbs on mb.position_code = mbs.position_code
left join lnucfloanobjective lno on ln.loanobjective_code = lno.loanobjective_code and ln.loantype_code = lno.loantype_code
left join lncfloanintratedet lnd on lnd.loanintrate_code = ln.int_continttabcode
and ln.startcont_date between lnd.effective_date and lnd.expire_date

left join mbucfprovince mbp on mb.province_code = mbp.province_code
left join mbucfdistrict mbd on mb.amphur_code = mbd.district_code and mb.province_code = mbd.province_code
left join mbucftambol mbt on mb.tambol_code = mbt.tambol_code and mb.amphur_code = mbt.district_code
left join amworkcalendar AA on AA.month = extract( month from ln.startcont_date  ) and  AA.year = extract( year from ln.startcont_date  ) +543

left join mbucfmembgroup mg on mb.membgroup_code = mg.membgroup_code
left join mbucfprovince mgp on mg.addr_province = mgp.province_code
left join mbucfdistrict mgd on mg.addr_amphur = mgd.district_code and mg.addr_province = mgd.province_code
left join mbucftambol mgt on mg.addr_tambol = mgt.tambol_code and mg.addr_amphur = mgt.district_code
left join amsecusers us on us.coop_id = ln.coop_id and user_name = $P{as_uesr}
where ln.coop_id = $P{as_coopid}
and $X{IN , ln.loancontract_no, list_loancontractno }
order by ln.loancontract_no]]>
	</queryString>
