	<queryString>
		<![CDATA[select
mm.member_no ,md.prename_desc||mm.memb_name ||' '||mm.memb_surname as memb_name ,mm.card_person
,mm.birth_date ,ftcalagemth( mm.birth_date , sysdate ) as ages_birthdate
,mm.member_date,ftcalagemth( mm.member_date , sysdate ) as ages_memberdate
,(ss.sharestk_amt * 10 ) as sharestk_amt
,mm.membgroup_code ,mg.membgroup_desc
,nvl(ln.principal_balance,0) as principal_balance
,nvl(kt.item_balance,0) as item_balance
from mbmembmaster mm
left join (select sum(principal_balance) as principal_balance ,member_no from lncontmaster  where contract_status = 1 and loantype_code = '20' group by member_no)ln on ln.member_no = mm.member_no
left join (select sum(item_balance ) as item_balance ,member_no  from kptempreceivedet where recv_period =(select max(recv_period) from kptempreceivedet) and keepitemtype_code like 'L%'  and shrlontype_code ='20' group by member_no) kt on kt.member_no = mm.member_no
left join mbucfmembgroup mg on  trim( mm.membgroup_code ) = trim( mg.membgroup_code )
left join mbucfprename md on mm.prename_code = md.prename_code
left join shsharemaster ss on mm.member_no = ss.member_no
where mm.coop_id = $P{as_coopid}
and trim(mm.member_no) between $P{as_smembno} and $P{as_emembno}
and mm.resign_status  <> 1
order by mm.membgroup_code , mm.member_no]]>
	</queryString>
