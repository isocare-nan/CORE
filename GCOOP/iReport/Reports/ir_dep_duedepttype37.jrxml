dpdeptmaster.deptaccount_name,
mbmembmaster.retry_date,
mbmembmaster.birth_date
from dpdeptmaster inner join mbmembmaster on dpdeptmaster.member_no = mbmembmaster.member_no
where dpdeptmaster.depttype_code='37' and dpdeptmaster.deptclose_status=0
