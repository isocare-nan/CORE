	<queryString>
		<![CDATA[select
LN.COOP_ID,LN.LOANREQUEST_DOCNO,
FTCNVTDATE(TO_DATE(LN.loanapprove_date),3) as startcont_date,
LN.MEMBER_NO,FT_GETMBNAME('018001',LN.MEMBER_NO) as getname,MP.position_desc,
TRIM(MB.MEMBGROUP_CODE) as MEMBGROUP_CODE,TRIM(GR.MEMBGROUP_DESC) as MEMBGROUP_DESC,GM.district_desc,
LN.loanrequest_amt as loanrequest_amt,FTREADTBAHT(LN.loanrequest_amt) as loanrequest_amt1,
FTREADTNUM(LN.PERIOD_PAYMENT) as PERIOD_PAYMENT,
FTREADTNUM(LN.PERIOD_PAYAMT) as PERIOD_PAYAMT,
LNU.LOANOBJECTIVE_DESC,LN.IDENTIFYCONT_INTRATE,LN.LOANCONTRACT_NO,
FTCNVTDATE(TO_DATE(LN.loanapprove_date),2) as loanapprove_date,
LN.loanapprove_amt as loanapprove_amt,FTREADTBAHT(LN.loanapprove_amt) as loanapprove_amt1,
(LN.loanapprove_amt - LN.loanrequest_amt) as add_amt,FTREADTBAHT(LN.loanapprove_amt - LN.loanrequest_amt) as add_amt1,
LC.ref_collno,FT_GETMBNAME('018001',LC.REF_COLLNO) as namecoll,
MBT.card_person,MP2.position_desc as MBT_pos,
ftcm_calagemth(MBT.birth_date,LN.loanapprove_date) as age,
FTCNVTDATE(MBT.birth_date,3) as birthdate,
MR1.membgroup_code as MR_code,MR1.membgroup_desc  as MR_desc,AM2.district_desc as MR_AM,
MBT.addr_no,MBT.addr_moo,MBT.addr_road,TM.tambol_desc,
AM.district_desc as AM_2 ,PM.province_desc,MBT.addr_phone,MBT.addr_mobilephone,MBT.addr_email,
 FT_GETINTRATE('018001' , lnloantype.inttabrate_code , LN.loanapprove_amt , LN.loanapprove_date ) as int_rate ,
(
case
when extract( month from ( case when extract(day from LN.loanapprove_date ) <= AA.postingdate then add_months( LN.loanapprove_date , 1)  else add_months( LN.loanapprove_date , 2) end ) )  = '1'  then 'มกราคม'
when extract( month from ( case when extract(day from LN.loanapprove_date ) <= AA.postingdate then add_months( LN.loanapprove_date , 1)  else add_months( LN.loanapprove_date , 2) end ) )  = '2'  then 'กุมภาพันธ์'
when extract( month from ( case when extract(day from LN.loanapprove_date ) <= AA.postingdate then add_months( LN.loanapprove_date , 1)  else add_months( LN.loanapprove_date , 2) end ) )  = '3'  then 'มีนาคม'
when extract( month from ( case when extract(day from LN.loanapprove_date ) <= AA.postingdate then add_months( LN.loanapprove_date , 1)  else add_months( LN.loanapprove_date , 2) end ) )  = '4'  then 'เมษายน'
when extract( month from ( case when extract(day from LN.loanapprove_date ) <= AA.postingdate then add_months( LN.loanapprove_date , 1)  else add_months( LN.loanapprove_date , 2) end ) )  = '5'  then 'พฤษภาคม'
when extract( month from ( case when extract(day from LN.loanapprove_date ) <= AA.postingdate then add_months( LN.loanapprove_date , 1)  else add_months( LN.loanapprove_date , 2) end ) )  = '6'  then 'มิถุนายน'
when extract( month from ( case when extract(day from LN.loanapprove_date ) <= AA.postingdate then add_months( LN.loanapprove_date , 1)  else add_months( LN.loanapprove_date , 2) end ) )  = '7'  then 'กรกฎาคม'
when extract( month from ( case when extract(day from LN.loanapprove_date ) <= AA.postingdate then add_months( LN.loanapprove_date , 1)  else add_months( LN.loanapprove_date , 2) end ) )  = '8'  then 'สิงหาคม'
when extract( month from ( case when extract(day from LN.loanapprove_date ) <= AA.postingdate then add_months( LN.loanapprove_date , 1)  else add_months( LN.loanapprove_date , 2) end ) )  = '9'  then 'กันยายน'
when extract( month from ( case when extract(day from LN.loanapprove_date ) <= AA.postingdate then add_months( LN.loanapprove_date , 1)  else add_months( LN.loanapprove_date , 2) end ) )  = '10'  then 'ตุลาคม'
when extract( month from ( case when extract(day from LN.loanapprove_date ) <= AA.postingdate then add_months( LN.loanapprove_date , 1)  else add_months( LN.loanapprove_date , 2) end ) )  = '11'  then 'พฤศจิกายน'
when extract( month from ( case when extract(day from LN.loanapprove_date ) <= AA.postingdate then add_months( LN.loanapprove_date , 1)  else add_months( LN.loanapprove_date , 2) end ) )  = '12'  then 'ธันวาคม'
end
) as month
,
( extract( year from ( case when extract(day from LN.loanapprove_date  ) <= 16 then add_months(  LN.loanapprove_date  , 1)  else add_months(  LN.loanapprove_date  , 1) end ) ) + 543 ) as year

from  LNCONTMASTER LN
left join amworkcalendar AA on AA.month = extract( month from LN.loanapprove_date  ) and  AA.year = extract( year from LN.loanapprove_date  ) +543
left join lnloantype on LN.loantype_code = lnloantype.loantype_code
left join lncontcoll LC on LC.loancontract_no = LN.loancontract_no
left join LNUCFLOANOBJECTIVE LNU on TRIM(LNU.LOANOBJECTIVE_CODE) = TRIM(LN.LOANOBJECTIVE_CODE) and TRIM(LNU.LOANTYPE_CODE) = TRIM(LN.LOANTYPE_CODE)
left join MBMEMBMASTER MB on LN.MEMBER_NO = MB.MEMBER_NO
left join MBMEMBMASTER MBT on TRIM(LC.REF_COLLNO) = MBT.MEMBER_NO
left join mbucfposition MP on MB.position_code = MP.position_code
left join MBUCFMEMBGROUP GR on TRIM(MB.MEMBGROUP_CODE) = TRIM(GR.MEMBGROUP_CODE)
left join MBUCFDISTRICT MST on MST.DISTRICT_CODE = GR.ADDR_AMPHUR
left join MBUCFDISTRICT GM on TRIM(GR.addr_amphur) = TRIM(GM.district_code)

left join MBUCFMEMBGROUP MR1 on TRIM(MBT.membgroup_code) = TRIM(MR1.membgroup_code)
left join MBUCFDISTRICT AM on TRIM(MBT.amphur_code) = TRIM(AM.district_code)
left join MBUCFDISTRICT AM2 on TRIM(MR1.addr_amphur) = TRIM(AM2.district_code)
left join MBUCFPROVINCE PM on TRIM(MBT.province_code) = TRIM(PM.province_code)
left join MBUCFTAMBOL TM on TRIM(MBT.tambol_code) = TRIM(TM.tambol_code)
left join mbucfposition MP2 on MBT.position_code = MP2.position_code

where LN.COOP_ID = $P{as_coopid}
and trim(LN.loancontract_no) = $P{as_loancontract_no}]]>
	</queryString>
