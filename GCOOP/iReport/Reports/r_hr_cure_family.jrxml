	<queryString>
		<![CDATA[select hr.emp_no as emp_no,mp.prename_desc||' '||hr.emp_name||' '||hr.emp_surname as fullname,
                            hp.pos_desc as position_emp,hr.salary_amt,
                            ha.assist_date,
                ha.assist_name,
                ha.assist_state,
ha.assist_fsalary,
                ha.assist_detail,
                ha.assist_hosname,
                ha.assist_sdate,
 ha.assist_date,
                ha.assist_edate,
                ha.assist_amt,
                ha.assist_minamt,
                ha.assist_posit,
                ha.assist_amp,
                ha.assist_prov,
                ha.assist_paper,
FTREADTBAHT( ha.assist_amt ) as total,
       FTREADTBAHT(ha.assist_minamt) as apptotal
                            from hremployee hr,mbucfprename mp,hrucfposition hp,hremployeeassist ha
                            where hr.prename_code = mp.prename_code
                            and hr.pos_code = hp.pos_code
                            and hr.emp_no = ha.emp_no
                            and ha.assist_code = '01'
                            and hr.coop_id = $P{as_coopid}
                            and substr(hr.emp_no,0,6) = $P{as_empno}
		       and ha.seq_no = $P{as_seqno}]]>
	</queryString>
