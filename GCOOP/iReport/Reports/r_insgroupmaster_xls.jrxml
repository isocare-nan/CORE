	<parameter name="as_period" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT
inm.member_no,
mp.prename_desc || ' ' ||mb.memb_name || ' ' || mb.memb_surname as fullname,
nvl((select sum(inm2.inscost_blance) from insgroupmaster inm2 where inm2.member_no = inm.member_no and inm2.endsafe_date = inm.endsafe_date and inm2.insmemb_status = 1 ),0) as sum_inscost_blanceall,
nvl((select sum(inm2.inscost_blance) from insgroupmaster inm2 where inm2.member_no = inm.member_no and inm2.endsafe_date = inm.endsafe_date and inm2.insmemb_status = 1 and inm2.loancontract_no like '%สว%'),0) as sum_inscost_blance01,
nvl((select sum(inm2.inscost_blance) from insgroupmaster inm2 where inm2.member_no = inm.member_no and inm2.endsafe_date = inm.endsafe_date and inm2.insmemb_status = 1 and inm2.loancontract_no like '%สร%'),0) as sum_inscost_blance02,
nvl((select sum(inm2.inscost_blance) from insgroupmaster inm2 where inm2.member_no = inm.member_no and inm2.endsafe_date = inm.endsafe_date and inm2.insmemb_status = 1 and (inm2.loancontract_no like 'สอ%' or inm2.loancontract_no like 'ฉก%' )),0) as sum_inscost_blance03,
nvl((select sum(inm2.inscost_blance) from insgroupmaster inm2 where inm2.member_no = inm.member_no and inm2.endsafe_date = inm.endsafe_date and inm2.insmemb_status = 1 and (inm2.loancontract_no like '%สม%' or inm2.loancontract_no like 'สห%' or inm2.loancontract_no like 'สศ%')),0) as sum_inscost_blance04,
nvl((select sum(inm2.inscost_blance) from insgroupmaster inm2 where inm2.member_no = inm.member_no and inm2.endsafe_date = inm.endsafe_date and inm2.insmemb_status = 1 and (inm2.loancontract_no like '%พศ%' or inm2.loancontract_no like 'พค%' )),0) as sum_inscost_blance05,
nvl((select sum(inm2.inscost_blance) from insgroupmaster inm2 where inm2.member_no = inm.member_no and inm2.endsafe_date = inm.endsafe_date and inm2.insmemb_status = 1 and inm2.loancontract_no like '%พพ%'),0) as sum_inscost_blance06,
nvl((select sum(inm2.inscost_blance) from insgroupmaster inm2 where inm2.member_no = inm.member_no and inm2.endsafe_date = inm.endsafe_date and inm2.insmemb_status = 1 and (inm2.loancontract_no like '%สน%' or inm2.loancontract_no like 'พน%' )),0) as sum_inscost_blance07
FROM
    insgroupmaster inm
left join mbmembmaster mb on mb.member_no = inm.member_no
left join mbucfprename mp on mp.prename_code = mb.prename_code
WHERE  inm.insmemb_status = 1
    AND inm.endsafe_date = (
        SELECT MAX(endsafe_date)
        FROM insgroupmaster inm1
        WHERE inm1.member_no = inm.member_no
        AND inm1.insmemb_status = 1
       AND EXTRACT(YEAR FROM inm.endsafe_date) = substr(   $P{as_period}  , 1, 4) - 542
    )
ORDER BY
    inm.member_no]]>
	</queryString>
