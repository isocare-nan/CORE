	<parameter name="coop_id" class="java.lang.String"/>
	<parameter name="start_membgroup" class="java.lang.String"/>
	<parameter name="end_membgroup" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
distinct(mbmembmaster.member_no),
mbmembmoneytr.bank_accid,
mbucfprename.prename_desc || '  ' || mbmembmaster.memb_name || '  ' || mbmembmaster.memb_surname as full_name,
mbmembmaster.card_person as Reference1
from mbmembmaster
left join mbucfprename on mbmembmaster.prename_code = mbucfprename.prename_code
left join mbmembmoneytr on mbmembmaster.member_no = mbmembmoneytr.member_no
where
mbmembmaster.membgroup_code between $P{start_membgroup} and $P{end_membgroup} and
mbmembmoneytr.trtype_code = 'KEEP2' and
mbmembmoneytr.bank_code = '006' and
mbmembmaster.coop_id = $P{coop_id}
order by mbmembmaster.member_no]]>
	</queryString>
	<field name="MEMBER_NO" class="java.lang.String"/>
	<field name="BANK_ACCID" class="java.lang.String"/>
	<field name="FULL_NAME" class="java.lang.String"/>
	<field name="REFERENCE1" class="java.lang.String"/>
	<variable name="COUNT_MEMB" class="java.math.BigDecimal" calculation="Count">
