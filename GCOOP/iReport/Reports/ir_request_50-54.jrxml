	<queryString>
		<![CDATA[select
    (select cc.coop_name from cmcoopconstant cc where mb.coop_id=cc.coop_no) coop_name,
    mb.member_no,
    ft_getmbname(mb.coop_id, mb.member_no) AS fullname,
    am.assisttype_code,
    ay.assisttype_desc,
	am.assistpay_code,
	ap.assistpay_desc,
    trunc(MONTHS_BETWEEN(sysdate, mb.birth_date) / 12) AS birth_date,
    trunc(MONTHS_BETWEEN(sysdate, mb.member_date) / 12) AS member_date,
    am.assist_amt,
			    am.remark
from assreqmaster am
join mbmembmaster mb on am.member_no = mb.member_no
join assucfassisttype ay on ay.assisttype_code=am.assisttype_code
join assucfassisttypepay ap on ap.assistpay_code=am.assistpay_code and ap.assisttype_code = am.assisttype_code
where am.req_date between $P{date1} and $P{date2}
and am.assisttype_code between $P{assisttype_start} and $P{assisttype_end}
and mb.resign_status = 0
and am.req_status > 0
order by am.assisttype_code,am.assistpay_code, am.req_date]]>
	</queryString>
