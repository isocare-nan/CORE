	<queryString>
		<![CDATA[select
slclsmthbalance.loancontract_no ,
lncontmaster.loanapprove_amt ,
slclsmthbalance.balance_value ,
lncontmaster.startcont_date ,
lncfloanintratedet.interest_rate
from slclsmthbalance
left join lncontmaster on slclsmthbalance.loancontract_no = lncontmaster.loancontract_no
left join lnloantype on lncontmaster.loantype_code = lnloantype.loantype_code
left join lncfloanintratedet on lnloantype.inttabrate_code = lncfloanintratedet.loanintrate_code and sysdate between lncfloanintratedet.effective_date and lncfloanintratedet.expire_date
where slclsmthbalance.clsmth_period = $P{recv_period}
and slclsmthbalance.bizz_system = 'LON'
and slclsmthbalance.member_no = $P{as_membno}]]>
	</queryString>
