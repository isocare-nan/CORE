	<queryString>
		<![CDATA[SELECT "SLSLIPPAYOUT"."SHRLONTYPE_CODE",
         "LNLOANTYPE"."PREFIX",
         "LNLOANTYPE"."LOANTYPE_DESC",
         sum( slslippayout.payout_amt ) as sum_loanrcv,
         count( slslippayout.loancontract_no ) as count_loanrcv
    FROM "SLSLIPPAYOUT",
         "LNLOANTYPE"
   WHERE ( "SLSLIPPAYOUT"."SHRLONTYPE_CODE" = "LNLOANTYPE"."LOANTYPE_CODE" )
         and ( "SLSLIPPAYOUT"."COOP_ID" = "LNLOANTYPE"."COOP_ID" )
         and ( ( slslippayout.coop_id = $P{as_coopid} )
		and ( slslippayout.slip_date between $P{adtm_startdate} and $P{adtm_enddate} )
         and ( slslippayout.shrlontype_code between $P{as_startlntype} and $P{as_endlntype} )
         and ( slslippayout.slip_status = 1 ) )
GROUP BY "SLSLIPPAYOUT"."SHRLONTYPE_CODE" ,
		"LNLOANTYPE"."PREFIX",
         "LNLOANTYPE"."LOANTYPE_DESC"]]>
	</queryString>
