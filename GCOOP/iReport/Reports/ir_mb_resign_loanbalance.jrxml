	<queryString>
		<![CDATA[select
mas.member_no,
mbucfprename.prename_desc,
mas.memb_name,
mas.memb_surname,
mas.resign_date,
mbucfmembgroup.membgroup_code,
mbucfmembgroup.membgroup_desc,
cmcoopmaster.coop_name,
sum(cls.balance_value),
cls.clsmth_period,
case trim(SUBSTR(cls.clsmth_period,5,6)) when '01' then 'มกราคม' when '02' then 'กุมภาพันธ์' when '03' then 'มีนาคม' when '04' then 'เมษายน' when '05' then 'พฤษภาคม' when '06' then 'มิถุนายน' when '07' then 'กรกฎาคม' when '08' then 'สิงหาคม' when '09' then 'กันยายน' when '10' then 'ตุลาคม' when '11' then 'พฤศจิกายน' when '12' then 'ธันวาคม'  else '' end  As mth
from slclsmthbalance cls
join mbmembmaster mas on mas.member_no = cls.member_no
left join  mbucfprename on mbucfprename.prename_code = mas.prename_code
left join  mbucfmembgroup on mbucfmembgroup.membgroup_code = mas.membgroup_code
left join  cmcoopmaster on mas.coop_id = cmcoopmaster.coop_id
where cls.coop_id = $P{as_coopid}
and cls.clsmth_period = $P{as_period}
and mas.resign_status = 1
and cls.bizz_system = 'LON'
group by mas.member_no,
mbucfprename.prename_desc,
mas.memb_name,
mas.memb_surname,
mas.resign_date,
mbucfmembgroup.membgroup_code,
mbucfmembgroup.membgroup_desc,
cmcoopmaster.coop_name,
cls.clsmth_period
order by mas.member_no
]]>
	</queryString>
