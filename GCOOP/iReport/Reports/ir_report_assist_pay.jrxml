	<queryString>
		<![CDATA[select ao.member_no,mb.prename_code,mb.memb_name,mb.memb_surname,ao.slip_date,ad.itempay_amt,ao.assisttype_code,
(select ut.assisttype_desc from assucfassisttype ut where ut.assisttype_code=ao.assisttype_code) assisttype_desc,
(select mp.prename_desc from mbucfprename mp where mb.prename_code=mp.prename_code)prename_desc,
(select cc.coop_name from cmcoopconstant cc where mb.coop_id=cc.coop_control) coop_name,
case when ad.deptaccount_no  is null then ad.expense_accid else ad.deptaccount_no  end as  deptaccount_no
 , ad.seq_no
from assslippayout ao
join mbmembmaster mb on ao.member_no=mb.member_no and ao.coop_id = mb.coop_id
left join assslippayoutdet ad on ao.assistslip_no = ad.assistslip_no
where ao.slip_date between $P{date1} and $P{date2} and ao.assisttype_code between $P{assisttype_start} and $P{assisttype_end} and ao.slip_status = 1 and ao.coop_id = $P{as_coopid}
order by ao.assisttype_code,ao.slip_date, ad.seq_no]]>
	</queryString>
